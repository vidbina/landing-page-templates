{
  "name": "landing-page-templates",
  "version": "0.1.0",
  "description": "Plain HTML+CSS templates of landing pages in the wild",
  "main": "index.js",
  "scripts": {
    "fmt": "npx prettier --write .",
    "capture": "node capture.js",
    "dev": "dev() { npx http-server $1 & npx node-sass -w $1/css -o $1/css; }; dev",
    "test": "npx prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vidbina/landing-page-templates.git"
  },
  "keywords": [
    "landing",
    "page",
    "templates",
    "html",
    "css",
    "static"
  ],
  "author": "David Asabina",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/vidbina/landing-page-templates/issues"
  },
  "homepage": "https://github.com/vidbina/landing-page-templates#readme",
  "dependencies": {
    "html5-boilerplate": "^7.3.0"
  },
  "devDependencies": {
    "http-server": "^0.12.1",
    "node-sass": "^7.0.0",
    "prettier": "^2.0.4",
    "puppeteer": "^3.0.0"
  }
}
